{
  "name": "webpack2-react-starter",
  "version": "0.0.1",
  "description": "Starting point for Webpack2 and React",
  "repository:": {
    "type": "git",
    "url": "https://github.com/nvandoorn/webpack2-react-starter"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build/*",
    "build": "npm run clean && NODE_ENV=production webpack --config ./config/webpack.config.js",
    "dev": "webpack-dev-server --config ./config/webpack.config.js --content-base build",
    "serve": "serve -s ./build"
  },
  "author": "Nick Van Doorn <vandoorn.nick@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-js-loader": "^0.3.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.17.4",
    "postcss-loader": "^2.0.5",
    "postcss-nesting": "^3.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "rimraf": "^2.6.1",
    "serve": "^5.1.5",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
